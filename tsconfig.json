{
    "compilerOptions": {
      "target": "ES6",
      "module": "ESNext",
      "outDir": "./",
      "moduleResolution": "Node",
      "declaration": true,
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true,
      "removeComments": true,
      "sourceMap": false,
      "noUnusedLocals": false,
      "noUnusedParameters": false,
      "noImplicitReturns": true,
      "allowJs": false,
      "charset": "utf-8"
    },
    "ts-node": {
        "compilerOptions": {
          "module": "commonjs"
        }
    },
    "include": [
      "src/**/*.ts"
    ],
  "exclude": []
}